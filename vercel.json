{
    "version": 2,
    "builds": [
        {
         "src": "myprofile/wsgi.py",
         "use": "@vercel/python",
         "config": { "maxLambdaSize": "15mb", "runtime": "python3.11" }
    },
    {
        "src": "builds_files.sh",
        "use": "@vercel/static_build",
        "config" :{
          "distDir": "static_build"
        }
    }
],
    "routes": [
        {
            "src":"/static/(.*)",
            "dest": "/static/$1"

        },
    {
    "src":"/(.*)",
    "dest": "myprofile/wsgi.py"
    }
    
],
"installCommand": "pip install -r requirements.txt"
}